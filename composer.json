{
    "name": "symfony-bundles/redis-bundle",
    "type": "symfony-bundle",
    "description": "Symfony Redis Bundle",
    "keywords": ["redis", "symfony", "bundle"],
    "homepage": "https://github.com/symfony-bundles/redis-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Dmitry Khaperets",
            "email": "khaperets@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "SymfonyBundles\\RedisBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "require": {
        "php": ">=5.6",
        "predis/predis": "~1.0",
        "symfony/framework-bundle": "~2.7|~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.3",
        "phpunit/php-code-coverage": "^3.3.0|^4.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
